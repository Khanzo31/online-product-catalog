{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "SKU": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Price": {
      "type": "decimal"
    },
    "Product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-type.product-type"
    },
    "CustomPropertyValues": {
      "type": "json"
    }
  }
}
